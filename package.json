{
  "name": "iot-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run": "npm run typeorm migration:run -- -d src/commons/configs/database.config.ts",
    "typeorm:generate": "npm run typeorm -- -d src/commons/configs/database.config.ts migration:generate src/database/migrations/$npm_config_name",
    "typeorm:create": "npm run typeorm -- migration:create src/database/migrations/$npm_config_name",
    "typeorm:revert": "npm run typeorm -- -d src/commons/configs/database.config.ts migration:revert"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.4.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "github:nestjs/mapped-types",
    "@nestjs/microservices": "^9.4.3",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/swagger": "github:nestjs/swagger",
    "@nestjs/typeorm": "^9.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/nodemailer": "^6.4.8",
    "@types/passport-jwt": "^3.0.8",
    "aedes": "^0.49.0",
    "aedes-server-factory": "^0.2.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "0.3.2",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.1.4",
    "handlebars": "^4.7.7",
    "mysql2": "^3.3.5",
    "nodemailer": "^6.9.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.0",
    "pigeon-mqtt-nest": "^1.0.17",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.8.0",
    "typeorm": "~0.3.16",
    "uuid": "^9.0.0",
    "webpack": "^5.87.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
